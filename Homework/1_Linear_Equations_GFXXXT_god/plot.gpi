set terminal pngcairo size 900,600 enhanced font 'Arial,12'
set output 'times_plot.png'

set title "QR-factorize an N×N matrix"
set xlabel "N"
set ylabel "Time [seconds]"
set grid

f(x) = c + a * x**3
c = 0.02
fit f(x) 'times.txt' using 1:2 via a, c

plot 'times.txt' using 1:2 with linespoints lt rgb "blue" lw 2 pt 7 title "Measured time", \
     f(x) with lines lt rgb "red" lw 2 title "Fit: c + a·N³"
